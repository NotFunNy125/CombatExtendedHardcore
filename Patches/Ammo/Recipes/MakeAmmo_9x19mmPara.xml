<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <!--Adjust 9x19 HP label-->
    <Operation Class="PatchOperationReplace">
        <xpath>Defs/RecipeDef[defName="MakeAmmo_9x19mmPara_HP"]/label</xpath>
            <value>
                <label>make 9x19mm Parabellum (HP) cartridge x20</label>
            </value>
    </Operation>
    <!--Adjust 9x19 HP description-->
    <Operation Class="PatchOperationReplace">
        <xpath>Defs/RecipeDef[defName="MakeAmmo_9x19mmPara_HP"]/description</xpath>
            <value>
                <description>Craft 20 9x19mm Parabellum (HP) cartridges.</description>
            </value>
    </Operation>
    <!--Adjust 9x19 HP ingredients-->
    <Operation Class="PatchOperationReplace">
        <xpath>Defs/RecipeDef[defName="MakeAmmo_9x19mmPara_HP"]/ingredients</xpath>
            <value>
                <ingredients>
                    <li>
                        <filter>
                            <thingDefs>
                                <li>Steel</li>
                            </thingDefs>
                        </filter>
                        <count>60</count>
                    </li>
                    <li>
                        <filter>
                            <thingDefs>
                                <li>FSX</li>
                            </thingDefs>
                        </filter>
                        <count>1</count>
                    </li>
                </ingredients>
            </value>
    </Operation>
    <!--Adjust 9x19 HP fixedIngredientFilter-->
    <Operation Class="PatchOperationReplace">
        <xpath>Defs/RecipeDef[defName="MakeAmmo_9x19mmPara_HP"]/fixedIngredientFilter</xpath>
            <value>
                <fixedIngredientFilter>
                    <thingDefs>
                        <li>Steel</li>
                        <li>FSX</li>
                    </thingDefs>
                </fixedIngredientFilter>
            </value>
    </Operation>
    <!--Adjust 9x19 HP products-->
    <Operation Class="PatchOperationReplace">
        <xpath>Defs/RecipeDef[defName="MakeAmmo_9x19mmPara_HP"]/products</xpath>
            <value>
                <products>
                    <Ammo_9x19mmPara_HP>20</Ammo_9x19mmPara_HP>
